		<display-name>SignIn</display-name>
		<description></description>
		<servlet-class>controller.SignIn</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ProductSearch</servlet-name>
		<display-name>ProductSearch</display-name>
		<description></description>
		<servlet-class>controller.ProductSearch</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ProductDetail</servlet-name>
		<display-name>ProductDetail</display-name>
		<description></description>
		<servlet-class>controller.ProductDetail</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>addProduct</servlet-name>
		<display-name>addProduct</display-name>
		<description></description>
		<servlet-class>controller.addProduct</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DeleteProduct</servlet-name>
		<display-name>DeleteProduct</display-name>
		<description></description>
		<servlet-class>controller.DeleteProduct</servlet-class>
	</servlet>
