		<description></description>
		<servlet-class>controller.ReservationProduit</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ValidateProduct</servlet-name>
		<display-name>ValidateProduct</display-name>
		<description></description>
		<servlet-class>controller.ValidateProduct</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UserHome</servlet-name>
		<display-name>UserHome</display-name>
		<description></description>
		<servlet-class>controller.UserHome</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SignIn</servlet-name>
		<url-pattern>/authen</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ProductSearch</servlet-name>
		<url-pattern>/ProductSearch</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ProductDetail</servlet-name>
		<url-pattern>/ProductDetail</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>addProduct</servlet-name>
		<url-pattern>/addProduct</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DeleteProduct</servlet-name>
		<url-pattern>/DeleteProduct</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SignUp</servlet-name>
		<url-pattern>/signup</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ReservationProduit</servlet-name>
		<url-pattern>/ReservationProduit</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ValidateProduct</servlet-name>
		<url-pattern>/ValidateProduct</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UserHome</servlet-name>
		<url-pattern>/UserHome</url-pattern>
