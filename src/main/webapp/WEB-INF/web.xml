<web-app>
	<display-name>Archetype Created Web Application</display-name>


	<filter>
		<filter-name>FilterHeadears</filter-name>
		<filter-class>filters.HeadersFilter</filter-class>
	</filter>

	<filter-mapping>
		<filter-name>FilterHeadears</filter-name>
